// C Programto find the cube if a number using function (no argument and no return type)

#include<stdio.h>
#include<math.h>
void high()
{
	double a;
	printf("Enter a nunber to find cube: ");
	scanf("%lf",&a);
	printf("The cube of given number : %.lf",pow(a,3));
}
void main()
{
	high(); 
}

